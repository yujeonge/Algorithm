package BOJ;

public class B1809_Moo {
   public static void main(String[] args) {
      StringBuilder sb = new StringBuilder();
      sb.append("(___)").append('\n');
      sb.append("(o o)____/").append('\n');
      sb.append(" @@      \\").append('\n');
      sb.append("  \\ ____,/").append('\n');
      sb.append("  //   //").append('\n');
      sb.append(" ^^   ^^").append('\n');
      
      System.out.println(sb);
   }
}
